<?xml version="1.0" encoding="utf-8"?>
<com.fast.libdeveloper.DebugDrawerLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/layout_debug_drawer"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:parentTag="com.fast.libdeveloper.DebugDrawerLayout">

  <FrameLayout
      android:id="@+id/content"
      android:layout_width="match_parent"
      android:layout_height="match_parent"
      />

  <include
      layout="@layout/layout_environment_config"
      android:id="@+id/layout_environment_config"
      android:layout_width="match_parent"
      android:layout_height="match_parent"
      android:layout_gravity="start"
      />

  <include
      layout="@layout/layout_log_print"
      android:id="@+id/layout_log_print"
      android:layout_width="match_parent"
      android:layout_height="match_parent"
      android:layout_gravity="end"
      />

</com.fast.libdeveloper.DebugDrawerLayout>
